		&-checkbox
			opacity 0
		
		&-text-ch
			cursor pointer
			position relative
			display inline-block
			vertical-align top
			padding-left 20px
			
			&::before
				content ''
				position absolute
				left 0
				top 50%
				margin-top -7px
				width 17px
				height 17px 
				background url('data:image/png;base64,') top no-repeat			
		
		& .active-ch
			&::before
				background-position 0 -18px
		
		&-sub
			border 0
			outline 0
			text-align center
			font-size 18px
			display inline-block
			vertical-align top
			color white
			padding 0 15px
			height 35px
			line-height 35px
			prefixProperty(border-radius, 8px)
			prefixProperty(transition, background .2s)
			background #24b651
			margin 0 auto
			text-transform uppercase
			
			a&
				background #0d98e2;
			
			a&:hover
				background #1aacfa;

			&:hover
				background #1ad353
		
		&-wr::before,
		&-wr::after
			content ''
			display table
		
		&-wr::after
			clear both			

		&-btn
			display inline-block
			vertical-align top
				
			&:last-child
				float right

