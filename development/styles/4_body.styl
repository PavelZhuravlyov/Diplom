	prefixProperty(transition, background .5s)
	padding 20px
	box-sizing border-box
	color #45494a
	background: #ae9f93;
	background: -moz-linear-gradient(top,  #ae9f93 0%, #dab797 34%, #efc090 68%, #eaaa5d 100%);
	background: -webkit-linear-gradient(top,  #ae9f93 0%,#dab797 34%,#efc090 68%,#eaaa5d 100%); 
	background: linear-gradient(to bottom,  #ae9f93 0%,#dab797 34%,#efc090 68%,#eaaa5d 100%); 

	&-head
		font-family "OpenSansBold"
		font-size 28px	
		text-align center
		margin-bottom 25px
	
	&-pref
		font-size 16px
		text-align center
		margin-bottom 20px
		
	&-f-l
		display block
		margin-bottom 15px
		
		&:before,
		&:after
			content ''
			display table
		
		&:after
			clear both	
					
		& table tr
						
		&-text
			display block
			
		&-inp
			display block
			width 100%
			height 30px
			line-height 30px
			font-size 16px
			color white
			background #666B6D
			
